{
  "coin": {
    "name": "eBoost",
    "shortcut": "EBST",
    "label": "eBoost",
    "alias": "eboost"
  },
  "ports": {
    "backend_rpc": 8065,
    "backend_message_queue": 38365,
    "blockbook_internal": 9065,
    "blockbook_public": 9165
  },
  "ipc": {
    "rpc_url_template": "http://127.0.0.1:{{.Ports.BackendRPC}}",
    "rpc_user": "rpc",
    "rpc_pass": "rpc",
    "rpc_timeout": 25,
    "message_queue_binding_template": "tcp://127.0.0.1:{{.Ports.BackendMessageQueue}}"
  },
  "backend": {
    "package_name": "backend-eboost", 
    "package_revision": "satoshilabs-1",
    "system_user": "eboost",
    "version": "2.3.3",
    "binary_url": "https://github.com/EBoost-dev/EBoost-dev/releases/download/v2.3.3/eboost-x86_64-unknown-linux-gnu.tar.gz",
    "verification_type": "sha256",
    "verification_source": "3fb0cf4b702b987173fcad8a3c5005f96e8c7372a713c53c3528221c7d7a998a",
    "extract_command": "tar -C backend --strip 1 -xf",
    "exclude_files": [
      "eboost-qt"
    ],
    "exec_command_template": "{{.Env.BackendInstallPath}}/{{.Coin.Alias}}/eboostd -datadir={{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend -conf={{.Env.BackendInstallPath}}/{{.Coin.Alias}}/{{.Coin.Alias}}.conf -pid=/run/{{.Coin.Alias}}/{{.Coin.Alias}}.pid",
    "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/*.log",
    "postinst_script_template": "",
    "service_type": "forking",
    "service_additional_params_template": "",
    "protect_memory": true,
    "mainnet": true,
    "server_config_file": "bitcoin_like.conf",
    "client_config_file": "bitcoin_like_client.conf",
    "additional_params": {
      "deprecatedrpc": "estimatefee"
    }
  },
  "blockbook": {
    "package_name": "blockbook-eboost",
    "system_user": "blockbook-eboost",
    "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
    "public_binding_template": ":{{.Ports.BlockbookPublic}}",
    "explorer_url": "",
    "additional_params": "-dbcache=1073741824",
    "block_chain": {
      "parse": true,
      "mempool_workers": 8,
      "mempool_sub_workers": 2,
      "block_addresses_to_keep": 300,
      "xpub_magic": 76067358,
      "slip44": 324,
      "additional_params": {
        "fiat_rates": "coingecko",
        "fiat_rates_params": "{\"url\": \"https://api.coingecko.com/api/v3\", \"coin\": \"eboost\", \"periodSeconds\": 60}"
      }
    }
  },
  "meta": {
    "package_maintainer": "IT",
    "package_maintainer_email": "it@satoshilabs.com"
  }
}
